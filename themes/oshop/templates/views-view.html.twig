          <div class="container">
            <div class="row text-center">
              <div class="col-lg-8 col-md-12 ml-auto mr-auto">
                <div class="section-title">
                  {{ footer }}
                </div>
                  {{ header }}
              </div>
            </div>
            <div class="row">
              <div class="col-lg-12 col-md-12">
                <div class="grid row columns-3">
                      {{ rows }}
                </div>
              </div>
            </div>
          </div>
        </section>
  {% elseif display_id == "top_on_sale"  %}
