           {{ header }}
            {{ rows }}
      </div>
  {% elseif display_id == "most_popular_home1"  %}
    <div class="col-lg-4 col-md-12 md-mt-3 sm-mt-5 update-mb4">
      {{ header }}
      {{ rows }}
   
    </div>
       {{ footer }}
  {% else %}
        <div class="row mb-5 align-items-center fix-mt-5">
